#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gokulkumar/swc-core/apps/api/node_modules/.pnpm/prisma@6.17.1_typescript@5.9.3/node_modules/prisma/build/node_modules:/Users/gokulkumar/swc-core/apps/api/node_modules/.pnpm/prisma@6.17.1_typescript@5.9.3/node_modules/prisma/node_modules:/Users/gokulkumar/swc-core/apps/api/node_modules/.pnpm/prisma@6.17.1_typescript@5.9.3/node_modules:/Users/gokulkumar/swc-core/apps/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gokulkumar/swc-core/apps/api/node_modules/.pnpm/prisma@6.17.1_typescript@5.9.3/node_modules/prisma/build/node_modules:/Users/gokulkumar/swc-core/apps/api/node_modules/.pnpm/prisma@6.17.1_typescript@5.9.3/node_modules/prisma/node_modules:/Users/gokulkumar/swc-core/apps/api/node_modules/.pnpm/prisma@6.17.1_typescript@5.9.3/node_modules:/Users/gokulkumar/swc-core/apps/api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../prisma@6.17.1_typescript@5.9.3/node_modules/prisma/build/index.js" "$@"
else
  exec node  "$basedir/../../../../../../prisma@6.17.1_typescript@5.9.3/node_modules/prisma/build/index.js" "$@"
fi
